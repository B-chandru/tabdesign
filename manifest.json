{
"manifest_version": 2,
"name" : "tabswe",
"description" : "Open beautiful new page",
  "version": "2.0",
  "icons":{
    "128":"images/icon128.png",
    "48":"images/icon48.png",
    "16":"images/icon16.png"
},
"browser_action":{
    "default_icon":{
        "128":"images/icon128.png",
        "48":"images/icon48.png",
        "16":"images/icon16.png"
    },
    "default_title":"Organise It"
},
"permissions": [
  "storage"
],
"chrome_url_overrides" : {
  "newtab":"index.html"
},
"content_scripts": [{
  "run_at": "document_start",
  "css": ["main.css"],
  "matches": [ "https://www.google.com/*" ]
}]    
}